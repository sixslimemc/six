// Removes duplicates from *items*, returning a set.

use super::super::super::def::Lambda1

type In<T> = struct {
    items: [T],
    // Use the result of this lambda on each element to compare equality.
    by?: Lambda1<T, any>,
    /// If true, preserve *items* order and prioritize first/left-most elements.
    /// Default: false
    ordered?: boolean,
}

type Out<T> = struct {
    result: [T]
}

/// Count of duplicates removed.
type Return = int @ 0..