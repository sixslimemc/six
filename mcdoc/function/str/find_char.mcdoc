// Finds the index of the first appearance of <char> in <string>.

struct In {
    /// The string to search in.
    str: string,
    /// The character to find.
    char: string @ 1,
    /// Whether or not escaped quote scopes should be accounted for (impacts performance).
    do_inner_escape: boolean
}

struct Out {}

/// 0..: <char> was found at this index in <string>.
/// -1: <string> did not contain <char>.
/// -2: <char> was not exactly 1 character.
type Return = int @ -2..